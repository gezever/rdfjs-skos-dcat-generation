@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#> .

        {
        ?a skos:broader ?b .
        ?b skos:broader ?c .
} => {
        ?a skos:broaderTransitive ?c .
} .

        {
        ?a skos:narrower ?b .
?b skos:narrower ?c .
} => {
        ?a skos:narrowerTransitive ?c .
} .

        {
        ?o skos:topConceptOf ?s .
} => {
        ?s skos:hasTopConcept ?o .
} .
        {
        ?o skos:hasTopConcept ?s .
} => {
        ?s skos:topConceptOf ?o .
} .

        {
        ?s skos:related ?o .
} => {
        ?o skos:related ?s .
} .

        {
        ?s skos:broader ?o .
} => {
        ?o skos:narrower ?s .
} .

        {
        ?s skos:narrower ?o .
} => {
        ?o skos:broader ?s .
} .

        {
        ?s skos:narrowerTransitive ?o .
} => {
        ?o skos:broaderTransitive ?s .
} .

        {
        ?s skos:broaderTransitive ?o .
} => {
        ?o skos:narrowerTransitive ?s .
        } .
